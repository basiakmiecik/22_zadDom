create database computer_games;

create table computer_games.games (
Id int auto_increment PRIMARY KEY,
Nazwa varchar(50) not null,
typ varchar(50) not null,
czas_gry int (4),
klas_wiek int (2),
ocena double
);

insert into computer_games.games (Nazwa, typ, czas_gry, klas_wiek, ocena)
